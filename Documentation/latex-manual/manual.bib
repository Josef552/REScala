@TECHREPORT{EPFL-REPORT-148043,
   abstract    = {Programming interactive systems by means of the observer
                 pattern is hard and error-prone yet is still the
                 implementation standard in many production environments.
                 We show how to integrate different reactive programming
                 abstractions into a single framework that help migrate
                 from observer-based event handling logic to more
                 declarative implementations. Our central API layer embeds
                 an extensible higher-order data-flow DSL into our host
                 language. This embedding is enabled by a continuation
                 passing style transformation.},
   affiliation = {EPFL},
   author      = {Maier, Ingo and Odersky, Martin},
   details     = {http://infoscience.epfl.ch/record/176887},
   documenturl = {http://infoscience.epfl.ch/record/176887/files/DeprecatingObservers2012.pdf},
   keywords    = {reactive programming; data flow; Scala},
   oai-id      = {oai:infoscience.epfl.ch:176887},
   oai-set     = {driver},
   pagecount   = {20},
   status      = {PUBLISHED},
   submitter   = {177179; 177179},
   title       = {Deprecating the {O}bserver {P}attern with {S}cala.React},
   unit        = {LAMP},
   year        = 2012
